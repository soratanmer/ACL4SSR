{
    "outbounds": [
      {
        "type": "direct",
        "tag": "direct"
      },
      {
        "type": "block",
        "tag": "block"
      }
    ],
    "route": {
      "rules": [
        {
          "type": "logical",
          "mode": "or",
          "rules": [
            {
              "protocol": "dns"
            },
            {
              "port": 53
            }
          ],
          "outbound": "dns"
        },
        {
          "ip_is_private": true,
          "outbound": "direct"
        },
        {
          "clash_mode": "Direct",
          "outbound": "direct"
        },
        {
          "clash_mode": "Global",
          "outbound": "default"
        },
        {
          "type": "logical",
          "mode": "or",
          "rules": [
            {
              "port": 853
            },
            {
              "network": "udp",
              "port": 443
            },
            {
              "protocol": "stun"
            }
          ],
          "outbound": "block"
        },
        {
          "rule_set": [
            "geoip-cn",
            "geosite-geolocation-cn"
          ],
          "outbound": "direct"
        }
      ],
      "rule_set": [
        {
          "type": "remote",
          "tag": "geoip-cn",
          "format": "binary",
          "url": "https://raw.githubusercontent.com/SagerNet/sing-geoip/rule-set/geoip-cn.srs"
        },
        {
          "type": "remote",
          "tag": "geosite-geolocation-cn",
          "format": "binary",
          "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-geolocation-cn.srs"
        }
      ]
    }
  }