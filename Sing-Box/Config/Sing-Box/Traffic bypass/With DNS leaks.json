{
    "dns": {
      "servers": [
        {
          "tag": "google",
          "address": "tls://8.8.8.8"
        },
        {
          "tag": "local",
          "address": "https://223.5.5.5/dns-query",
          "detour": "direct"
        }
      ],
      "rules": [
        {
          "outbound": "any",
          "server": "local"
        },
        {
          "clash_mode": "Direct",
          "server": "local"
        },
        {
          "clash_mode": "Global",
          "server": "google"
        },
        {
          "rule_set": "geosite-geolocation-cn",
          "server": "local"
        },
        {
          "clash_mode": "Default",
          "server": "google"
        },
        {
          "type": "logical",
          "mode": "and",
          "rules": [
            {
              "rule_set": "geosite-geolocation-!cn",
              "invert": true
            },
            {
              "rule_set": "geoip-cn"
            }
          ],
          "server": "local"
        }
      ]
    },
    "route": {
      "rule_set": [
        {
          "type": "remote",
          "tag": "geosite-geolocation-cn",
          "format": "binary",
          "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-geolocation-cn.srs"
        },
        {
          "type": "remote",
          "tag": "geosite-geolocation-!cn",
          "format": "binary",
          "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-geolocation-!cn.srs"
        },
        {
          "type": "remote",
          "tag": "geoip-cn",
          "format": "binary",
          "url": "https://raw.githubusercontent.com/SagerNet/sing-geoip/rule-set/geoip-cn.srs"
        }
      ]
    },
    "experimental": {
      "cache_file": {
        "enabled": true,
        "store_rdrc": true
      },
      "clash_api": {
        "default_mode": "Enhanced"
      }
    }
  }